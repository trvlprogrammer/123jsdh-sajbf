<odoo>
	<data>
		<record id="cron_report_creator_15_minutes" forcecreate='True' model="ir.cron">
	      <field name="name">Report Creator 15 Minutes</field>	      
	      <field eval="True" name="active"/>
	      <field name="user_id" ref="base.user_root"/>
	      <field name="interval_number">15</field>
	      <field name="interval_type">minutes</field>
	      <field name="numbercall">-1</field>
	      <field name="model_id" ref="model_report_result"/>
	      <field name="state">code</field>
	      <field name="code">model.schedule_15_minutes()</field>
		</record>
		
