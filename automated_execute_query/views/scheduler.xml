		</record>
		
		<record id="cron_execute_querymonthly" forcecreate='True' model="ir.cron">
	      <field name="name">Execute Query Monthly</field>	      
	      <field eval="True" name="active"/>
	      <field name="user_id" ref="base.user_root"/>
